{
  "id": "deltaverse-prod-env",
  "name": "DeltaVerse Production",
  "values": [
    {
      "key": "base_url",
      "value": "https://api.deltaverse.app",
      "description": "Production API server",
      "enabled": true
    },
    {
      "key": "fi_mcp_url",
      "value": "https://fi-mcp.deltaverse.app",
      "description": "Production Fi-MCP server",
      "enabled": true
    },
    {
      "key": "firebase_token",
      "value": "{{$randomUUID}}",
      "description": "Real Firebase JWT token (replace with actual)",
      "enabled": true
    },
    {
      "key": "user_id",
      "value": "{{$randomUUID}}",
      "description": "Real user ID",
      "enabled": true
    },
    {
      "key": "environment",
      "value": "production",
      "description": "Current environment",
      "enabled": true
    },
    {
      "key": "debug_mode",
      "value": "false",
      "description": "Disable debug logging in production",
      "enabled": true
    }
  ],
  "_postman_variable_scope": "environment"
}
